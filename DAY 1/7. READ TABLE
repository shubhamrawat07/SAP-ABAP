REPORT z_practice_08.

DATA gt_numbers TYPE STANDARD TABLE OF i.
DATA gv_num TYPE i.

APPEND 100 TO gt_numbers.
APPEND 200 TO gt_numbers.

READ TABLE gt_numbers INTO gv_num INDEX 2.            (index just for sy-subrc)

IF sy-subrc = 0.
  WRITE: / 'Found:', gv_num.
ELSE.
  WRITE:/ 'Not Found'.                        (only for index 2+ will be invalid as only till index 2 data is present)
ENDIF.
